public static Integer arraySum(List<Integer> numbers) {
    //code here
    /*if(numbers!=null){
      Integer sum=0;
      for(Integer i=0;i<numbers.size();i++){
    sum+=numbers[i];    
      }  
      return sum; 
    }else{
        return null;
    } */
    Integer Sum=0;
    if(numbers.isEmpty()){
        return 0;
    }else{
        for(Integer num:numbers){
            Sum+=num;
        }
        return Sum;
    }


}
