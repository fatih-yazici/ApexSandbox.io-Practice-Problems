public Boolean isBirthday(Contact c) {

    //code here
    Integer cM = c.Birthdate.Month();//cM c Month
    Integer cD = c.Birthdate.Day(); //cD c Day

    Date t = system.today();
    Integer tM = t.Month(); // tM tMonth
    Integer tD = t.Day(); // tD t Day

    If(cM==tM && cD==tD){
        return true;
    }else{
        return false;
    }


}
